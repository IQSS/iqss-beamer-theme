% Requirement
\RequirePackage{tikz}
\RequirePackage{amsmath}
\RequirePackage{unicode-math}

\mode<presentation>

\setbeamertemplate{footline}[frame number]
\setbeamertemplate{navigation symbols}{}
\setbeamertemplate{blocks}[rounded][shadow=true]

\newcommand{\alertb}[1]{\textcolor{iqss@blue}{#1}}
\newcommand{\alertc}[1]{\textcolor{iqss@purple}{#1}}
\newcommand{\alertd}[1]{\textcolor{iqss@green}{#1}}
\newcommand{\alerte}[1]{\textcolor{iqss@red}{#1}}
\newcommand{\alertf}[1]{\textcolor{iqss@yellow}{#1}}


\usefonttheme{iqss}
\usecolortheme{iqss}
\useinnertheme{default}
\useoutertheme{default}

\mode
<all>
